                ],
                "summary": "Deletar um nível",
                "operationId": "95b63514dd90008c4185702e4c5f9ea9",
                "parameters": [
                    {
                        "name": "id",
                        "in": "path",
                        "description": "ID do nível",
                        "required": true,
                        "schema": {
                            "type": "integer"
                        }
                    }
                ],
                "responses": {
                    "204": {
                        "description": "Nível deletado com sucesso"
                    },
                    "400": {
                        "description": "Não é possível deletar um nível que possui desenvolvedores associados"
                    },
                    "404": {
                        "description": "Nível não encontrado"
                    }
                }
            }
        }
