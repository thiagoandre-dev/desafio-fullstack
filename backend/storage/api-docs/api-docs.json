            "email": "thiago.andre.ti@gmail.com"
        },
        "version": "1.0.0"
    },
    "paths": {
        "/api/desenvolvedores": {
            "get": {
                "tags": [
                    "Desenvolvedores"
                ],
                "summary": "Listar desenvolvedores",
                "operationId": "59ee2c434a8f547891b5d76ab82e9757",
                "parameters": [
                    {
                        "name": "nivel_id",
                        "in": "query",
                        "description": "Filtro pelo ID do nível",
                        "required": false,
                        "schema": {
                            "type": "integer"
                        }
                    },
                    {
                        "name": "nome",
                        "in": "query",
                        "description": "Filtro pelo nome do desenvolvedor",
                        "required": false,
                        "schema": {
                            "type": "string"
                        }
                    },
                    {
                        "name": "sexo",
                        "in": "query",
                        "description": "Filtro pelo sexo do desenvolvedor (M ou F)",
                        "required": false,
                        "schema": {
                            "type": "string",
                            "enum": [
                                "M",
                                "F"
                            ]
                        }
                    },
                    {
                        "name": "hobby",
                        "in": "query",
                        "description": "Filtro pelo hobby do desenvolvedor",
                        "required": false,
                        "schema": {
                            "type": "string"
                        }
                    },
                    {
                        "name": "data_nascimento",
                        "in": "query",
                        "description": "Filtro pela data de nascimento do desenvolvedor (YYYY-MM-DD)",
                        "required": false,
                        "schema": {
                            "type": "string",
                            "format": "date"
                        }
                    },
                    {
                        "name": "page",
                        "in": "query",
                        "description": "Número da página para paginação",
                        "required": false,
                        "schema": {
                            "type": "integer",
                            "default": 1
                        }
                    },
                    {
                        "name": "limit",
                        "in": "query",
                        "description": "Número de itens por página para paginação",
                        "required": false,
                        "schema": {
                            "type": "integer",
                            "default": 10
                        }
                    },
                    {
                        "name": "order_by",
                        "in": "query",
                        "description": "Campo para ordenar os resultados (id, nome, nivel, sexo, data_nascimento)",
                        "required": false,
                        "schema": {
                            "type": "string",
                            "default": "id"
                        }
                    },
                    {
                        "name": "order_direction",
                        "in": "query",
                        "description": "Direção da ordenação (asc ou desc)",
                        "required": false,
                        "schema": {
                            "type": "string",
                            "default": "asc",
                            "enum": [
                                "asc",
                                "desc"
                            ]
                        }
                    }
                ],
                "responses": {
                    "200": {
                        "description": "Lista de desenvolvedores retornada com sucesso"
                    }
                }
            },
            "post": {
                "tags": [
                    "Desenvolvedores"
                ],
                "summary": "Criar um novo desenvolvedor",
                "operationId": "79d206e8107cf767aacd707922a55048",
                "requestBody": {
                    "required": true,
                    "content": {
                        "application/json": {
                            "schema": {
                                "required": [
                                    "nivel_id",
                                    "nome",
                                    "sexo",
                                    "data_nascimento"
                                ],
                                "properties": {
                                    "nivel_id": {
                                        "type": "integer",
                                        "example": 1
                                    },
                                    "nome": {
                                        "type": "string",
                                        "maxLength": 255,
                                        "example": "João Silva"
                                    },
                                    "sexo": {
                                        "type": "string",
                                        "enum": [
                                            "M",
                                            "F"
                                        ],
                                        "example": "M"
                                    },
                                    "hobby": {
                                        "type": "string",
                                        "maxLength": 255,
                                        "example": "Futebol"
                                    },
                                    "data_nascimento": {
                                        "type": "string",
                                        "format": "date",
                                        "example": "1990-05-15"
                                    }
                                },
                                "type": "object"
                            }
                        }
                    }
                },
                "responses": {
                    "201": {
                        "description": "Desenvolvedor criado com sucesso"
                    },
                    "422": {
                        "description": "Erro de validação"
                    }
                }
            }
        },
        "/api/desenvolvedores/{id}": {
            "get": {
                "tags": [
                    "Desenvolvedores"
                ],
                "summary": "Obter detalhes de um desenvolvedor",
                "operationId": "0316a08cbe3c5a219cd11204dd95e1fd",
                "parameters": [
                    {
                        "name": "id",
                        "in": "path",
                        "description": "ID do desenvolvedor",
                        "required": true,
                        "schema": {
                            "type": "integer"
                        }
                    }
                ],
                "responses": {
                    "200": {
                        "description": "Detalhes do desenvolvedor retornados com sucesso"
                    },
                    "404": {
                        "description": "Desenvolvedor não encontrado"
                    }
                }
            },
            "put": {
                "tags": [
                    "Desenvolvedores"
                ],
                "summary": "Atualizar um desenvolvedor existente",
                "operationId": "f19a4f4f281c735290a0cd2e43516916",
                "parameters": [
                    {
                        "name": "id",
                        "in": "path",
                        "description": "ID do desenvolvedor",
                        "required": true,
                        "schema": {
                            "type": "integer"
                        }
                    }
                ],
                "requestBody": {
                    "required": true,
                    "content": {
                        "application/json": {
                            "schema": {
                                "required": [
                                    "nivel_id",
                                    "nome",
                                    "sexo",
                                    "data_nascimento"
                                ],
                                "properties": {
                                    "nivel_id": {
                                        "type": "integer",
                                        "example": 2
                                    },
                                    "nome": {
                                        "type": "string",
                                        "maxLength": 255,
                                        "example": "Maria Souza"
                                    },
                                    "sexo": {
                                        "type": "string",
                                        "enum": [
                                            "M",
                                            "F"
                                        ],
                                        "example": "F"
                                    },
                                    "hobby": {
                                        "type": "string",
                                        "maxLength": 255,
                                        "example": "Leitura"
                                    },
                                    "data_nascimento": {
                                        "type": "string",
                                        "format": "date",
                                        "example": "1985-10-20"
                                    }
                                },
                                "type": "object"
                            }
                        }
                    }
                },
                "responses": {
                    "200": {
                        "description": "Desenvolvedor atualizado com sucesso"
                    },
                    "404": {
                        "description": "Desenvolvedor não encontrado"
                    },
                    "422": {
                        "description": "Erro de validação"
                    }
                }
            },
            "delete": {
                "tags": [
                    "Desenvolvedores"
                ],
                "summary": "Deletar um desenvolvedor",
                "operationId": "ab714e67b8b76ccf2e0f7799ae557b43",
                "parameters": [
                    {
                        "name": "id",
                        "in": "path",
                        "description": "ID do desenvolvedor",
                        "required": true,
                        "schema": {
                            "type": "integer"
                        }
                    }
                ],
                "responses": {
                    "204": {
                        "description": "Desenvolvedor deletado com sucesso"
                    },
                    "404": {
                        "description": "Desenvolvedor não encontrado"
                    }
                }
            }
        },
        "/api/niveis": {
            "get": {
                "tags": [
                    "Níveis"
                ],
                "summary": "Listar níveis",
                "operationId": "f983113f46d3765dfcacc06fe03b483c",
                "parameters": [
                    {
                        "name": "nivel",
